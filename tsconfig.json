{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["dom", "es2018"],
    "jsx": "react",
    "outDir": "./dist",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}


